                        },
                        "options": {
                            "type": "object",
                            "title": "Options",
                            "properties": {
                                "hwaddress": {
                                    "type": "string",
                                    "title": "MAC address",
                                    "pattern": "^(?:[0-9A-Fa-f]{2}:){5}[0-9A-Fa-f]{2}$",
                                    "propertyOrder": 11
                                },
                                "mtu": {
                                    "type": "string",
                                    "title": "MTU",
                                    "propertyOrder": 12
                                }

        "method_nm_ethernet": {
            "title": "Ethernet",
            "type": "object",
            "description": "Managed by NetworkManager",
            "properties": {
                "name": {
                    "type": "string",
                    "title": "Connection name",
                    "minLength": 1,
                    "propertyOrder": 1
                },
                "auto": {
                    "type": "boolean",
                    "default": true,
                    "title": "Auto",
                    "_format": "checkbox",
                    "propertyOrder": 2
                },
                "device": {
                    "type": "string",
                    "title": "Only for device",
                    "propertyOrder": 3,
                    "options": {
                        "inputAttributes": {
                            "placeholder":  "use with any suitable device"
                        },
                        "wb": {
                            "show_editor": true
                        }
                    },
                    "watch": {
                        "devices": "data.devices.ethernet"
                    },
                    "enumSource": [
                        {
                            "source": "devices"
                        }
                    ],
                    "_format": "edWb"
                },
                "method": {
                    "type": "string",
                    "title": "Method",
                    "options": {
                        "hidden": true
                    },
                    "enum": ["01_nm_ethernet"]
                },
                "uuid": {
                    "type": "string",
                    "options": {
                        "hidden": true
                    },
                    "default": ""
                },
                "hwaddress": {
                    "type": "string",
                    "title": "MAC address",
                    "pattern": "^(?:[0-9A-Fa-f]{2}:){5}[0-9A-Fa-f]{2}$",
                    "propertyOrder": 16,
                    "options": {
                        "show_opt_in": true
                    }
                },
                "mtu": {
                    "type": "integer",
                    "title": "MTU",
                    "default": 1500,
                    "propertyOrder": 18,
                    "options": {
                        "inputAttributes": {
                            "placeholder":  " "
                        },
                        "show_opt_in": true
                    }
                },
                "ipv4": {
                    "title": "IPv4",
                    "oneOf": [
                        {"$ref": "#/definitions/nm_ipv4_auto"},
                        {"$ref": "#/definitions/nm_ipv4_manual"}
                    ],
                    "propertyOrder": 4,
                    "options": {
                        "keep_oneof_values": false
                    }
                }
            },
            "required": ["method", "name", "uuid", "ipv4"],
            "options": {
                "disable_edit_json": true,
                "disable_properties": true,
                "wb": {
                    "disable_panel": true,
                    "icon": "ethernet"
                }
            }
        },

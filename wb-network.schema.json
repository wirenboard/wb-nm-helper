    "properties": {
        "ui": {
            "type": "object",
            "properties": {
                "interfaces": {
                    "type": "array",
                    "title": "Network connections",
                    "items": {
                        "title": "connection_title",
                        "headerTemplate": "{{self.name}}",
                        "oneOf": [
                            { "$ref": "#/definitions/method_nm_ethernet" },
                            { "$ref": "#/definitions/method_nm_wifi" },
                            { "$ref": "#/definitions/method_nm_modem" },
                            { "$ref": "#/definitions/method_loopback" },
                            { "$ref": "#/definitions/method_static" },
                            { "$ref": "#/definitions/method_dhcp" },
                            { "$ref": "#/definitions/method_ppp" },
                            { "$ref": "#/definitions/method_can" },
                            { "$ref": "#/definitions/method_manual" }
                        ],
                        "options": {
                            "keep_oneof_values": false,
                            "disable_collapse": true
                        },
                        "_format": "wb-multiple"
                    },
                    "propertyOrder": 1,
                    "options": {
                        "array_controls_top": true,
                        "disable_array_delete_last_row": true,
                        "disable_array_reorder": true,
                        "disable_collapse": true,
                        "disable_array_delete_all_rows": true,
                        "wb": {
                            "select_label": "Choose connection type",
                            "sort_by_property": "method"
                        }
                    },
                    "_format": "wb-array"
                },
                "con_switch": {
                    "title": "Automatic connections switching",
                    "type": "object",
                    "propertyOrder": 2,
                    "description": "con_switch_desc",
                    "properties": {
                        "connections": {
                            "title": "Connections in descending order of priority",
                            "type": "array",
                            "uniqueItems": true,
                            "propertyOrder": 2,
                            "items": {
                                "title": "connection_title",
                                "type": "string",
                                "minLength": 1,
                                "watch": {
                                    "interfaces": "ui.interfaces"
                                },
                                "enumSource": [
                                    {
                                        "source": "interfaces",
                                        "filter": "{{if item.method in [\"01_nm_ethernet\",\"02_nm_modem\",\"03_nm_wifi\"]}}1{{endif}}{{endif}}",
                                        "title": "{{item.name}}",
                                        "value": "{{item.name}}"
                                    }
                                ],
                                "_format": "edWb"
                            },
                            "options": {
